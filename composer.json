{
    "name": "mediactive-digital/medkit",
    "description": "Package to init a ready-to-dev Laravel application",
    "license": "MIT",
    "authors": [
        {
            "name": "Mediactive Digital",
            "email": "dev@mediactive.fr",
            "homepage": "https://www.mediactive-digital.com/"
        }
    ],
    "homepage": "https://github.com/mediactive-digital/medkit",
    "keywords": ["Laravel", "MedKit", "boilerplate"],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10",
        "illuminate/support": "^10",
        "laravel-lang/lang": "^12.16",
        "kris/laravel-form-builder": "^1.52",
        "lavary/laravel-menu": "^1.8.3",
        "spatie/laravel-permission": "^5.8",
        "wildside/userstamps": "^2.3",
        "mediactive-digital/laravel-gettext": "dev-master",
        "ext-json": "*",
        "genealabs/laravel-model-caching": "*",
        "ext-pdo": "*",
        "sentry/sentry-laravel": "*",
        "soved/laravel-gdpr": "^1.7",
        "infyomlabs/laravel-generator": "9.x-dev",
        "funkjedi/composer-include-files": "^1.1",
        "composer/composer": "^2.5",
        "yajra/laravel-datatables-oracle": "^10.3",
        "yajra/laravel-datatables-buttons": "^10",
        "sepia/po-parser": "^6.0.1",
        "tightenco/ziggy": "^1.5",
        "spatie/laravel-database-mail-templates": "^3.5.1",
        "spatie/laravel-translatable": "^6.3",
        "predis/predis": "^2.1",
        "doctrine/dbal": "^3.6",
        "rairlie/laravel-locking-session": "^2",
        "laravel/ui": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mockery/mockery": "*",
        "orchestra/testbench": "*",
        "sempro/phpunit-pretty-print": "*"
    },
    "autoload": {
        "psr-4": {
            "MediactiveDigital\\MedKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MediactiveDigital\\MedKit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MediactiveDigital\\MedKit\\MedKitServiceProvider",
                "Sentry\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "MedKit": "MediactiveDigital\\MedKit\\Facades\\MedKit",
                "Sentry": "Sentry\\Laravel\\Facade"
            }
        }
    }
}
